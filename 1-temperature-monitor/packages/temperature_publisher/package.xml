<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>temperature_publisher</name>
  <version>0.1.0</version>
  <description>A ROS 2 node that publishes simulated temperature data.</description>

  <maintainer email="yarashahinstem@gmail.com">Yara Shahin</maintainer>
  <maintainer email="mahmoud3taha@gmail.com">Mahmoud Taha</maintainer>

  <license>Mozilla Public License 2.0</license>

  <buildtool_depend>ament_python</buildtool_depend>
  <depend>rclpy</depend>
  <depend>temperature_monitoring_interfaces</depend>

  <test_depend>ament_copyright</test_depend>
  <test_depend>ament_flake8</test_depend>
  <test_depend>ament_pep257</test_depend>
  <test_depend>python3-pytest</test_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
